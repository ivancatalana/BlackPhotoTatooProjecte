<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileFragment">

    <ImageView
        android:id="@+id/friendProfileImageView"
        android:layout_width="120dp"
        android:layout_height="120dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/followers"
        android:layout_width="88dp"
        android:layout_height="32dp"
        android:fontFamily="@font/reem_kufi"
        android:gravity="center|top"
        android:text="00"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toStartOf="@+id/floowing"
        app:layout_constraintStart_toEndOf="@+id/publishedImages" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="98dp"
        android:layout_height="31dp"
        android:fontFamily="@font/reem_kufi"
        android:gravity="center"
        android:text="Followers"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/textView11"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        tools:layout_editor_absoluteY="201dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="99dp"
        android:layout_height="47dp"
        android:fontFamily="@font/reem_kufi"
        android:gravity="center"
        android:text="Following"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="95dp"
        android:layout_height="56dp"
        android:fontFamily="@font/reem_kufi"
        android:gravity="bottom"
        android:text="Published Images"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="parent"
        tools:textColor="@color/black" />

    <TextView
        android:id="@+id/nameProfile"
        android:layout_width="198dp"
        android:layout_height="54dp"
        android:fontFamily="@font/rocksalt"
        android:gravity="center"
        android:text="UserName"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/friendProfileImageView" />

    <TextView
        android:id="@+id/publishedImages"
        android:layout_width="58dp"
        android:layout_height="41dp"
        android:fontFamily="@font/reem_kufi"
        android:gravity="bottom|center"
        android:text="00"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/floowing"
        android:layout_width="81dp"
        android:layout_height="32dp"
        android:fontFamily="@font/reem_kufi"
        android:gravity="center|top"
        android:text="00"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerProfile"
        android:layout_width="358dp"
        android:layout_height="405dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

</androidx.constraintlayout.widget.ConstraintLayout>
